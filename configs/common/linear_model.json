{
    "PARAMETERS_SETS": {
        "common linear parameters": {
            "algorithm": {
                "estimator": "LinearRegression",
                "estimator_params": { "fit_intercept": true, "copy_X": true }
            }
        },
        "common ridge parameters": {
            "algorithm": {
                "estimator": "Ridge",
                "estimator_params": {
                    "fit_intercept": true,
                    "alpha": 2.0
                }
            }
        },
