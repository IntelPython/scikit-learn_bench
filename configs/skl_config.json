        {
            "algorithm": "svm",
            "dataset": [
                {
                    "source": "csv",
                    "name": "a9a",
                    "training":
                    {
                        "x": "data/a9a_x_train.csv",
                        "y": "data/a9a_y_train.csv"
                    },
                    "testing":
                    {
                        "x": "data/a9a_x_test.csv",
                        "y": "data/a9a_y_test.csv"
                    }
                }
            ],
            "C": [500.0],
            "kernel": ["rbf"]
