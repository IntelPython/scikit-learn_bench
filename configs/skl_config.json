{
    "omp_env": ["OMP_NUM_THREADS"],
    "common": {
        "lib": ["sklearn"],
        "data-format": ["pandas"],
        "data-order": ["F"],
        "dtype": ["float64"]
    },
    "cases": [
        {
            "algorithm": "kmeans",
            "dataset": [
                {
                    "source": "synthetic",
                    "type": "blobs",
                    "n_clusters": 1000,
                    "n_features": 20,
                    "training": {
                        "n_samples": 1000000
                    }
                }
